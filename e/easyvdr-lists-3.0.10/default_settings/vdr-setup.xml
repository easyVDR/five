                            <value>DSL3000</value>
                            <value>DSL6000</value>
                            <value>DSL16000</value>
                            <value>WLAN11</value>
                            <value>WLAN54</value>
                            <value>LAN10</value>
                        </entry>
                    </menu>
                    <menu name="Parameter und Einstellungen">
