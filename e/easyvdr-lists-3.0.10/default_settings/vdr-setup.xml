                        <entry name="Standard-Verzeichnis" sysconfig="FTPD_DEFAULT_DIR" type="text" value="/media/easyvdr01/" />
                        <entry name="FTP-Port" sysconfig="FTPD_PORT" type="number" value="21" />
                        <entry name="Aktivitäten loggen" sysconfig="FTPD_LOG" type="bool" value="no" />
                    </menu>
                    <menu name="Streamdev" >
