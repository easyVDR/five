 adjust-alsamixer		= no
 quit-program			= yes
 pre-start 			= "no"
 start-program			= "$START_PROGRAM $UTILITIES start_firefox www.spiegel.de &"
 start-program-easyportal	= "$START_PROGRAM_EASYPORTAL $UTILITIES start_firefox www.spiegel.de &"
 stop-program			= "firefox"
 kill-program			= yes
 root-rights			= no
 vdr-remote-on			= no
 install-pakage			= "firefox firefox-locale-de"
end-config

