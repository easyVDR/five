start-config
 button-name			= Gianas-Return 
 button-picture 		= /usr/share/easyvdr/program-changer/pictures/button-gianas-return.png
 default-button			= yes
 default-button-easyportal	= no
 mouse-emulation		= yes
 menu-page 			= 1
 adjust-alsamixer		= yes
 quit-program			= yes 
 pre-start 			= "cd /opt;[ ! -e giana ] && wget http://www.retroguru.com/gianas-return/gianas-return-v.latest-linux.tar.gz;gunzip gianas-return-v.latest-linux.tar.gz;tar -xf *.tar;$UTILITIES init_gaming_mod"
 start-program			= "cd /opt/giana ;$START_PROGRAM ./giana_linux64 -fs -aa -b24 -a44 &"
 start-program-easyportal	= "cd /opt/giana ;$START_PROGRAM_EASYPORTAL ./giana_linux64 -fs -aa -b24 -a44 &"
 stop-program			= "giana_linux64"
 kill-program			= yes
 root-rights			= no
 vdr-remote-on			= no
 install-pakage			= "freeglut3 libsdl-mixer1.2  $PULSEAUDIO_INSTALL $GAMES_INSTALL"
end-config		

