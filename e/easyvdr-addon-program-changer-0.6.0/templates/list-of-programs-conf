#Die Version wird im Setup ausgewertet, ist in der datei /config/version eine andere Version wird die Datei config/setup.conf gelöscht.
#Es werden alle default Einstellungen wie bei einer neu Installation übernommen, Ausnahme ist die list-of-programs-user.conf
Version = 1.1


start-config  
 button-name			= Vdr-Frontend
 button-picture 		= /usr/share/easyvdr/program-changer/pictures/button-vdr.png
 default-button			= yes
 default-button-easyportal	= yes
 mouse-emulation		= no
 menu-page 			= 1
 adjust-alsamixer		= no
 quit-program			= yes 
 pre-start 			= "no"
 start-program			= "easyvdr-frontend start"
 start-program-easyportal	= "easyvdr-frontend start"
 stop-program			= "easyvdr-frontend stop"
 kill-program			= no
 root-rights			= yes
 vdr-remote-on			= yes
 install-pakage			= "-none-"
end-config		

start-config 
 button-name			= KODI 
 button-picture 		= /usr/share/easyvdr/program-changer/pictures/button-kodi.png
 default-button			= yes
 default-button-easyportal	= yes
 mouse-emulation		= no
 menu-page 			= 1
 adjust-alsamixer		= no
 quit-program			= yes 
 pre-start 			= "/usr/lib/vdr/easyvdr-kodi-change-skins vdr_shutdown"
 start-program			= "easyvdr-kodi start"
 start-program-easyportal	= "echo KODI > /tmp/.pch-exec"
 stop-program			= "easyvdr-kodi stop"
 kill-program			= no
 root-rights			= yes
 vdr-remote-on			= no
 install-pakage			= "kodi $PULSEAUDIO_INSTALL"
end-config		

start-config 
 button-name			= KODI&WOL-Server
 button-picture 		= /usr/share/easyvdr/program-changer/pictures/button-kodi.png
 default-button			= no
 default-button-easyportal	= no
 mouse-emulation		= no
 menu-page 			= 1
 adjust-alsamixer		= no
 quit-program			= yes 
 pre-start 			= "$UTILITIES wol_server &"
 start-program			= "easyvdr-kodi start"
 start-program-easyportal	= "echo KODI > /tmp/.pch-exec"
 stop-program			= "easyvdr-kodi stop"
 kill-program			= no
 root-rights			= yes
 vdr-remote-on			= no
 install-pakage			= "kodi etherwake $PULSEAUDIO_INSTALL"
end-config		

start-config
 button-name			= Firefox 
 button-picture 		= /usr/share/easyvdr/program-changer/pictures/button-firefox.png
 default-button			= yes
 default-button-easyportal	= no
 mouse-emulation		= yes
 menu-page 			= 1
 adjust-alsamixer		= yes
 quit-program			= yes 
 pre-start 			= "no"
 start-program			= "$START_PROGRAM firefox -browser -new-instance &"
 start-program-easyportal	= "$START_PROGRAM_EASYPORTAL firefox -browser -new-instance &"
 stop-program			= "firefox"
 kill-program			= yes
 root-rights			= no
 vdr-remote-on			= no
 install-pakage			= "firefox firefox-locale-de $PULSEAUDIO_INSTALL"
end-config		


################ Google-Chrome

start-config
 button-name			= Google-Chrome
 button-picture			= /usr/share/easyvdr/program-changer/pictures/button-google-chrome.png
 default-button			= no
 default-button-easyportal	= no
 mouse-emulation		= yes
 menu-page			= 1
 adjust-alsamixer		= yes
 quit-program			= yes
 pre-start			= "$UTILITIES install_google_chrome $1"
 start-program			= "$START_PROGRAM google-chrome -browser -new-instance &"
 start-program-easyportal	= "$START_PROGRAM_EASYPORTAL google-chrome -browser -new-instance &"
 stop-program			= "chrome"
 kill-program			= yes
 root-rights			= no
 vdr-remote-on			= no
 install-pakage			= "-none-"
end-config

